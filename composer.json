{
    "name": "taghassan54/laravel-arabic-numbers",
    "description": "This package allows you to fix Arabic numbers input .",
    "keywords": [
        "laravel",
        "middleware",
        "arabic",
        "numbers",
        "taghassan54",
        "taghassan",
        "taghassan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "taghassan54",
            "email": "taghassan54@gmail.com",
            "homepage": "https://github.com/taghassan54",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.75"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.1",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TagHassan54\\ArabicNumbers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TagHassan54\\ArabicNumbers\\Test\\": "tests"
        }
    }, "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "vendor/bin/phpcs --standard=psr2 src/",
        "fix-style": "vendor/bin/phpcbf --standard=psr2 src/",
        "clover": "vendor/bin/codacycoverage clover"
    },
    "config": {
        "sort-packages": true
    }
}
